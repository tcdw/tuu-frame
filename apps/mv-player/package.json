  "dependencies": {
    "@mtv/shared-api-types": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "bcrypt": "^6.0.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "electron-vite": "^2.3.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "globals": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "react-player": "^2.16.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.30.1",
